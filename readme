new table employees id name age
new table salaries id salary department

insert employees 1 John 30
insert employees 2 Tom 20
insert employees 3 Amy 25

insert salaries 1 50000 Engineering
insert salaries 2 55000 Marketing
insert salaries 3 52000 Marketing2

give name,age from table employees where age >= 25
delete from employees where age = 30
update employees set name=John,age=35 where id = 1
order employees by id desc
aggregate sum from employees on age

join employees and salaries id print employees.name



